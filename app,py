    # Create output directory if it doesn't exist
    if not os.path.exists(output_dir):
        os.makedirs(output_dir)

    count = 0
    for div_tag in soup.find_all('div', class_='BreakpointGrid_item__RSMyf', limit=limit):
        # print(soup.find_all('div', class_='BreakpointGrid_item__RSMyf', limit=limit))
        article_tag = div_tag.find('article', class_='MediaCard_card__6_MG7')
        # print(article_tag)
        if article_tag:
            a_tag = article_tag.find('a', class_='Link_link__Ime8c spacing_noMargin__F5u9R')
            if a_tag:
                img_tag = a_tag.find('img')
                if img_tag:
                    img_url = img_tag.get('src')
                    if img_url:
                        img_data = requests.get(img_url).content
                        img_name = os.path.join(output_dir, f'image_{count + 1}.jpg')

                        with open(img_name, 'wb') as handler:
                            handler.write(img_data)

                        count += 1
                        print(f'Downloaded {img_name}')
                    else:
                        print("No valid image URL found in src attribute.")
                else:
                    print("No img tag found within the a tag.")
            else:
                print("No a tag found within the article.")
        else:
            print("No article tag found within the div.")

